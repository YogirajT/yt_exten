{

  "manifest_version": 2,
  "name": "YT Exten",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/ytn-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["assets/js/jquery/v3.6.4.js", "index.js"]
    }
  ]

}
